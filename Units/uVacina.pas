unit uVacina;

interface
  uses
    Vcl.ExtCtrls;


  Type TVacina = class

    private
       idVacina : integer;
       dataVacina : String;
       horaVacina : String;
       nomeVacina : String;
       localVacina : String;
       nomeProfissional : String;
       responsavel : String;
       proximaAplicacao : String;
       idCrianca : integer;

    public
        procedure setIdVacina(idVacina : integer);
        function getIdVacina : integer;

        procedure setDataVacina(DataVacina : String);
        function getDataVacina : String;

        procedure setHoraVacina(HoraVacina : String);
        function getHoraVacina : String;

        procedure setNomeVacina(NomeVacina : String);
        function getNomeVacina : String;

        procedure setLocalVacina(LocalVacina : String);
        function getLocalVacina : String;

        procedure setNomeProfissional(NomeProfissional : String);
        function getNomeProfissional : String;

        procedure setResponsavel(Responsavel : String);
        function getResponsavel : String;

        procedure setProximaAplicacao(ProximaAplicacao : String);
        function getProximaAplicacao : String;

        procedure setIdCrianca(idCrianca : integer);
        function getIdCrianca : integer;

  end;

implementation

{ TVacina }

function TVacina.getDataVacina: String;
begin
  result := self.dataVacina;
end;

function TVacina.getHoraVacina: String;
begin
  result := self.horaVacina;
end;

function TVacina.getIdCrianca: integer;
begin
  result := self.idCrianca;
end;

function TVacina.getIdVacina: integer;
begin
  result := self.idVacina;
end;

function TVacina.getLocalVacina: String;
begin
  result := self.localVacina;
end;

function TVacina.getNomeProfissional: String;
begin
  result := self.nomeProfissional;
end;

function TVacina.getNomeVacina: String;
begin
  result := self.nomeVacina;
end;

function TVacina.getProximaAplicacao: String;
begin
  result := self.proximaAplicacao;
end;

function TVacina.getResponsavel: String;
begin
  result := self.responsavel;
end;

procedure TVacina.setDataVacina(DataVacina: String);
begin
  self.dataVacina := DataVacina;
end;

procedure TVacina.setHoraVacina(HoraVacina: String);
begin
  self.horaVacina := HoraVacina;
end;

procedure TVacina.setIdCrianca(idCrianca: integer);
begin
  self.idCrianca := idCrianca;
end;

procedure TVacina.setIdVacina(idVacina : integer);
begin
  self.idVacina :=  idVacina;
end;

procedure TVacina.setLocalVacina(LocalVacina: String);
begin
  self.localVacina := LocalVacina;
end;

procedure TVacina.setNomeProfissional(NomeProfissional: String);
begin
  self.nomeProfissional := NomeProfissional;
end;

procedure TVacina.setNomeVacina(NomeVacina: String);
begin
  self.nomeVacina := NomeVacina;
end;

procedure TVacina.setProximaAplicacao(ProximaAplicacao: String);
begin
  self.proximaAplicacao := ProximaAplicacao;
end;

procedure TVacina.setResponsavel(Responsavel: String);
begin
  self.responsavel := Responsavel;
end;

end.
